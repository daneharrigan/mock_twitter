[
{% repeat %}
   {
      "contributors":null,
      "in_reply_to_screen_name":"{{ in_reply_to_screen_name }}",
      "truncated":false,
      "created_at":"{{ created_at }}",
      "favorited":false,
      "source":"MockTwitter",
      "geo":null,
      "in_reply_to_user_id":"{{ in_reply_to_user_id }}",
      "user":{
         "profile_sidebar_fill_color":"252429",
         "description":"{{ user_description }}",
         "time_zone":"Eastern Time (US & Canada)",
         "favourites_count":{{ user_favourites_count }},
         "created_at":"{{ user_created_at }}",
         "profile_sidebar_border_color":"181A1E",
         "statuses_count":{{ user_statuses_count }},
         "profile_image_url":"http://a3.twimg.com/profile_images/214167439/avatar_normal.jpg",
         "contributors_enabled":false,
         "profile_text_color":"666666",
         "lang":"en",
         "notifications":false,
         "profile_background_image_url":"http://s.twimg.com/a/1267135446/images/themes/theme9/bg.gif",
         "friends_count":{{ user_friends_count }},
         "screen_name":"{{ in_reply_to_screen_name }}",
         "following":false,
         "profile_link_color":"2FC2EF",
         "protected":false,
         "geo_enabled":false,
         "profile_background_tile":false,
         "location":"{{ user_location }}",
         "name":"{{ user_name }}",
         "profile_background_color":"1A1B1F",
         "id":{{ in_reply_to_user_id }},
         "verified":false,
         "utc_offset":-18000,
         "followers_count":{{ user_followers_count }},
         "url":"{{ user_url }}"
      },
      "in_reply_to_status_id":{{ in_reply_to_status_id }},
